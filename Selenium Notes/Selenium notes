package locators;

import java.util.List;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class Flipkart {

	public static void main(String[] args) throws InterruptedException {

		WebDriver driver = new ChromeDriver();
		driver.get("https://www.flipkart.com/");
		driver.manage().window().maximize();
		
		//search product
		driver.findElement(By.xpath("//input[@name='q']")).sendKeys("iPhone" + Keys.ENTER);
		
//		driver.findElement(By.xpath("(//div[contains(@class,'_4rR01T')])[1]")).click();
		
		//--------------------------------------------------------------------------------------
		
		List<WebElement> allproducts = driver.findElements(By.xpath("//div[contains(@class,'_4rR01T')]"));
		
		allproducts.get(23).click();
		
		String text = allproducts.get(0).getText();
		System.out.println(text);

		for(WebElement product:allproducts)
		{
			String text1 = product.getText();
			System.out.println(text1);
		}
		
	}

}

