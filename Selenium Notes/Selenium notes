package popupHandling;

import java.util.ArrayList;
import java.util.Set;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Flipkart {

	public static void main(String[] args) throws InterruptedException {
		WebDriver driver = new ChromeDriver();
		driver.get("https://www.flipkart.com/");
		driver.manage().window().maximize();
		
		//search product
		driver.findElement(By.xpath("//input[@name='q']")).sendKeys("iPhone" + Keys.ENTER);
		driver.findElement(By.xpath("(//div[contains(@class,'_4rR01T')])[1]")).click();
		
		
		//get ids 
		String parent_id = driver.getWindowHandle();
		System.out.println(parent_id);
		
		Set<String> all_ids = driver.getWindowHandles();
		System.out.println(all_ids);
		
		ArrayList<String> al = new ArrayList<>(all_ids);
		
		System.out.println(al.get(0));
		System.out.println(al.get(1));
		
		//-----------------------------------------------
		
		driver.switchTo().window(al.get(1));
		System.out.println(driver.getTitle());
		
		Thread.sleep(3000);
		driver.findElement(By.xpath("//button[text()='Add to cart']")).click();
		
		driver.switchTo().window(parent_id);
		System.out.println(driver.getTitle());

	}

}

